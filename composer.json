{
    "name": "ruudk/absurd-php-sdk",
    "description": "PHP SDK for Absurd: a Durable Execution Engine for Postgres",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ruudk\\Absurd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ruudk\\Absurd\\": "tests/",
            "Ruudk\\Absurd\\Examples\\": "examples/"
        }
    },
    "authors": [
        {
            "name": "Ruud Kamphuis",
            "email": "ruudk@users.noreply.github.com"
        }
    ],
    "require": {
        "symfony/dotenv": "^7.4 || ^8.0",
        "php": "^8.4",
        "ext-pdo": "*",
        "ext-pcntl": "*",
        "psr/log": "^3.0",
        "psr/container": "^2.0",
        "symfony/serializer": "^7.4 || ^8.0",
        "symfony/property-access": "^7.4 || ^8.0",
        "symfony/event-dispatcher": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "carthage-software/mago": "^1.1.0",
        "symfony/console": "^7.4 || ^8.0",
        "monolog/monolog": "^3.10"
    },
    "config": {
        "allow-plugins": {
            "carthage-software/mago": true
        }
    }
}
